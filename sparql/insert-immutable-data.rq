# To add non-writeable items in to newly defined "immutable"
# which is done by INSERT DATA (insert-immutable.rq)
# thus performing a simple inference
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX cim: <https://sites.google.com/site/ontology2013/>
PREFIX imm: <http://www.semanticweb.org/ontologies/2013/immutable#>
INSERT
{
  ?x rdf:type imm:immutable
}
WHERE
{
  ?x cim:Writeable "false"^^xsd:boolean
}